                /if (${ConditionsOn} && ${DPSCOn} && ${If[${DPSCond[${i}]},0,1]}) /break
                /if (${ConditionsOn} && ${DPSCOn} && ${DPSPart1.Equal[command]}) {
                    /if (${Bool[${DPSPart4}]}) {
                        /docommand /${DPSPart3} ${DPSPart4}
