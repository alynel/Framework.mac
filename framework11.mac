                } else /if (${MacroReturn.Equal[CAST_TAKEHOLD]}) {
                    /echo ** ${DPSPart1} on >> ${Spawn[${DPSTargetID}].CleanName} << - DID NOT TAKE HOLD
                    /varset DPSTimer${i} 5m
                }
            }    
            | This is the Bottom of the Outer Loop. Add additional checks needed before /next i entry.
            /if (${DebugCombat}) /echo \atDEBUGCOMBAT CombatCast ${DPS[${i}].Arg[1,|]} ${Macro.Return} DPSTimer${i} ${DPSTimer${i}} \agLine#: ${Macro.CurLine}
            /if (${Select[${DPSPart3},Me,MA]}) /call CombatTargetCheck
            /if (${WeaveArray.Size}>0 && ${WeaveArray[1].NotEqual[null]} && (${Me.SpellInCooldown} || !${WeaveCheck})) /call WeaveStuff ${DPSTargetID}
            /if (${MashArray.Size}>0 && ${MashArray[1].NotEqual[null]}) /call MashButtons
            /varset WeaveCheck 0
        /next i
        /if (${DebugCombat}) /echo \atDEBUGCOMBAT CombatCast: Leave \agLine#: ${Macro.CurLine}
    /return 
