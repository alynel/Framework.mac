                    /if (${DebugCombat}) /echo \atDEBUGCOMBAT CombatCast  DPSTarget: ${DPSTargetID} ${Spawn[${DPSTargetID}]} End: ${DPSSkip} ${Spawn[${DPSTargetID}].PctHPs} Start: ${DPSAt} Target: ${Target.ID} Timer: ${DPSTimer${i}} DPSOn: ${DPSOn} \agLine#: ${Macro.CurLine}
                    /if (${Select[${DPSPart3},Me,MA]} && ${Me.Combat} && ${MainAssist.NotEqual[${Me}]}) {
                        /attack off
                        /delay 10 !${Me.Combat}
                    }
